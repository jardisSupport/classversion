{
    "name": "jardissupport/classversion",
    "description": "ClassVersion enables loading classes with the same name from specific subdirectories associated with respective version labels",
    "type": "library",
    "license": "PolyForm-Noncommercial-1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Headgent Development",
            "email": "jardisDev@headgent.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "jardisport/classversion": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.0.4",
        "squizlabs/php_codesniffer": "^3.11.2 || ^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "JardisSupport\\ClassVersion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JardisSupport\\ClassVersion\\Tests\\": "tests/"
        }
    },
    "homepage": "https://github.com/jardissupport/classversion",
    "support": {
        "issues": "https://github.com/jardissupport/classversion/issues",
        "email": "jardisDev@headgent.com"
    },
    "keywords": [
        "versioning",
        "classversion",
        "version class loader",
        "loader",
        "domain driven design",
        "jardissupport",
        "jardis",
        "headgent"
    ]
}
